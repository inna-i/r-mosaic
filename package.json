{
  "name": "page",
  "version": "1.0.0",
  "description": "My page",
  "license": "Apache-2.0",
  "author": "Artem",
  "homepage": "https://r4v3nn.github.io/Github-users/",
  "engines": {
    "npm": "6.10.1",
    "node": "10.16.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.js"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "roots": [
      "src"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/scripts/**"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "testEnvironment": "jest-environment-jsdom-global",
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/actions",
      "<rootDir>/src/components/index.js",
      "<rootDir>/src/config",
      "<rootDir>/src/containers/index.js",
      "<rootDir>/src/reducers/index.js",
      "<rootDir>/src/sagas/index.js",
      "<rootDir>/src/store"
    ]
  },
  "scripts": {
    "deploy": "npm run webpack-build && gh-pages -d dist",
    "postinstall": "npm rebuild node-sass",
    "lint-es": "eslint --config .eslintrc src/**/*",
    "lint-sass": "sass-lint -q -v",
    "lint": "npm run lint-es && npm run lint-sass",
    "start": "node server.js",
    "start-dev" : "yarn webpack-dev-server --mode development --open",
    "debug": "cross-env NODE_ENV=production node $NODE_DEBUG_OPTION ./node_modules/.bin/webpack-dev-server",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "webpack-prod": "cross-env NODE_ENV=production webpack --mode production",
    "build": "npm run webpack-prod",
    "webpack-tests": "npm run test:cov",
    "webpack-analyze": "cross-env NODE_ENV=production webpack --env.analyze --display-error-details --display-cached",
    "webpack": "cross-env NODE_ENV=development webpack --mode development",
    "extract-i18n-iam-xtm": "i18next-scanner --config i18next-scanner-iam-xtm.config.js 'src/**/*.js'",
    "extract-18n-and-copy": "npm run extract-i18n-iam-xtm && npm run copy-i18n-to-local",
    "copy-i18n-to-local": "copyfiles -u 1 \"./i18n/iam-app.json\" \"./src/locales/en\""
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "acorn": "^6.1.0",
    "autoprefixer": "^7.1.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.0",
    "compression-webpack-plugin": "0.4.0",
    "copy-webpack-plugin": "^4.6.0",
    "copyfiles": "2.0.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "fetch-mock": "^5.13.1",
    "file-loader": "^2.0.0",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^3.2.0",
    "i18next-parser": "^0.13.0",
    "i18next-scanner": "2.10.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "^24.0.0",
    "jest-cli": "^24.0.0",
    "jest-environment-jsdom": "^24.0.0",
    "jest-environment-jsdom-global": "1.1.0",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "node-zopfli": "2.0.3",
    "nodemon": "^1.19.1",
    "postcss-loader": "^3.0.0",
    "redux-mock-store": "^1.2.3",
    "resolve-url-loader": "3.0.0",
    "sass-lint": "1.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "0.4.6",
    "underscore-template-loader": "^1.0.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.0",
    "webpack-bundle-analyzer": "^3.0.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.21.2"
  },
  "dependencies": {
    "@talend/bootstrap-theme": "3.1.0",
    "@talend/icons": "3.1.0",
    "@talend/react-components": "3.1.0",
    "@talend/react-containers": "3.1.0",
    "@talend/react-forms": "3.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.0.0",
    "classnames": "^2.2.5",
    "country-list": "1.1.0",
    "cross-env": "^5.2.0",
    "eslint-loader": "^2.2.1",
    "express": "4.17.1",
    "history": "3.0.0",
    "i18next": "^9.0.0",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-xhr-backend": "^1.4.3",
    "js-cookie": "2.1.4",
    "lodash": "^4.17.4",
    "lscache": "^1.1.0",
    "path": "^0.12.7",
    "prop-types": "^15.5.10",
    "react": "^16.8.0",
    "react-addons-css-transition-group": "15.6.2",
    "react-async-script": "^0.9.1",
    "react-bootstrap": "0.32.4",
    "react-css-transition": "0.7.4",
    "react-dom": "^16.8.0",
    "react-ga": "2.5.7",
    "react-helmet": "5.2.0",
    "react-i18next": "7.10.1",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.0",
    "react-router-dom": "^5.0.0",
    "react-router-redux": "^4.0.8",
    "react-transition-group": "^2.3.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "4.2.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "request": "2.88.0",
    "urijs": "1.18.10",
    "whatwg-fetch": "^2.0.3"
  }
}
